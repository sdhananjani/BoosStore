{
  "Serilog": {
    "Minimumlevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"

      }
    },
    "WriteTo": [
      {

        "Name": "File",
        "Args": {
          "path": "./logs/log_.txt",
          "rollingInterval": "Day"
        }

      }
    ]
  },
  "AllowedHosts": "*"
}
